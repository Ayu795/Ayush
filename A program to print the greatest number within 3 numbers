public class greatestN
{
	public static void main(String[] args) {
	
		int x=15;
			int y=6;
				int z=7;
				if(x>y){
				  	if(x>z){
				System.out.println("x is greatest");	    
			}else{
	System.out.println("z is greatest");			    
			}  
				}
				else if(y>x){
				    	if(y>z){
		System.out.println("y is greatest");			    
			}else{
	System.out.println("z is greatest");			    
			}  
				}
	}
}
